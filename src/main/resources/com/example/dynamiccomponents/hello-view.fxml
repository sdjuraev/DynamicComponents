<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.dynamiccomponents.HelloController" prefWidth="400" prefHeight="400">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <FlowPane fx:id="paneView" orientation="HORIZONTAL" alignment="BASELINE_CENTER" vgap="20" hgap="30">

    </FlowPane>
<TextArea fx:id="textArea"></TextArea>
    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
    <Button text="Cancel" fx:id="cancelButton" onAction="#cancelWindow"></Button>
    <Button text="Read JSON STREAM" fx:id="jsonReader" onAction="#setJsonReader"></Button>


</VBox>
